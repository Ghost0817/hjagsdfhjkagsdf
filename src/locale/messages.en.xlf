        <source>Include two-factor authentication</source>
        <context-group purpose="location">
          <context context-type="sourcefile">src/app/pages/randompasswordgenerate/index/index.component.html</context>
          <context context-type="linenumber">199</context>
        </context-group>
      </trans-unit>
      <trans-unit id="026e0cf6045786f5210bc834487d41a9ee7d4913" datatype="html">
