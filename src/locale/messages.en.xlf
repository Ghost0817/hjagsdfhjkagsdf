      <trans-unit id="0f714755ad005984d374405040cf14544a0f77b8" datatype="html">
        <source>Be sure to link your mobile phone to your account, if the service allows. At the moment, this is the most reliable way to recover a password.</source>
        <context-group purpose="location">
          <context context-type="sourcefile">src/app/pages/randompasswordgenerate/index/index.component.html</context>
